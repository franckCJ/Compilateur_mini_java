class A
{
	Int local_value=3

	Int calc_value(Int temp_value, Boolean exact){
		if (exact) (
			temp_value
		)
		else (
			local_value = 1
		)
	}
	
	Int call(){
		this.calc_value(local_value,true)
	}
}

class B extends A {
	Int calc_value(Int temp_value, Boolean exact){
		if (!exact) (
			temp_value
			)
		else (
			local_value = 5
		)
	}
}

A atest = new A in {
	atest.call();
	B btest = new B in {
		btest.calc_value(10,true)
	}
}

